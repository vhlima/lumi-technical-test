{
  "name": "lumi-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --transpile-only --ignore-watch node_modules src/index.ts",
    "migration:run": "dotenv -- typeorm-ts-node-esm migration:run -d src/infra/data-sources/postgres.ts",
    "test": "jest",
    "build": "tsc --outDir build -p . --esModuleInterop --experimentalDecorators --resolveJsonModule --lib es6 --module commonjs",
    "start": "node --loader ts-paths-esm-loader build/src/index.js"
  },
  "dependencies": {
    "celebrate": "^15.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "pdf.js": "^0.1.0",
    "pdfjs-dist": "^3.8.162",
    "pg": "^8.11.1",
    "reflect-metadata": "^0.1.13",
    "ts-paths-esm-loader": "^1.4.3",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.17",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.4",
    "@types/pdfjs-dist": "^2.10.378",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
